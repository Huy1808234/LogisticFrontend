    <section id="doc-slides">
      <div class="container">

        <h3>Slides</h3>

        <figure class="screenshot no-border w-1-3">
          <img src="/img/screen/slide-controls.png" alt="Slide change, remove and add controls along with image and caption tabs" />
          <span class="marker" style="top:27%;left:43%;">1</span>
          <span class="marker" style="top:27%;right:30%;">2</span>
          <span class="marker" style="bottom:12%;left:45%;">3</span>
